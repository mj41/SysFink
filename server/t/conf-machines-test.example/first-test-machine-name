hostname    first-test-machine-name.my-domain.com
dist_type   linux-perl-md5
ssh_user    root
exclude     /dev/ /home/ /proc/ /sys/ /var/ /mnt/

[fastscan]
exclude     /
include     /etc

[testscanA]
exclude     /
include     /root/sysfink-test-dir

[testscanB]
exclude     /
path        [#-B5]/root/sysfink-test-dir

[tmpscan]
exclude     /
path        [#-B5]/tmp
